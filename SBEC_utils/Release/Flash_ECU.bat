@ECHO OFF
SETLOCAL
SBEC_chipid.exe /c:1
IF %ERRORLEVEL% EQU 8360 GOTO FLASH_ST
IF %ERRORLEVEL% EQU 417 GOTO FLASH_ST
IF %ERRORLEVEL% EQU 35257 GOTO FLASH_ST
IF %ERRORLEVEL% EQU 38976 GOTO FLASH_TOSH
GOTO QUIT

:FLASH_ST
SBEC_erase_st.exe /c:1
IF %ERRORLEVEL% NEQ 0 GOTO QUIT
SBEC_flash_st.exe /c:1 /f:..\..\56028828\56028828.bin
IF %ERRORLEVEL% NEQ 0 GOTO QUIT
SBEC_eeflash.exe /C:1 /F:..\..\MCU_eeprom\56028828_eeprom.bin /e
GOTO QUIT

:FLASH_TOSH
SBEC_erase_tosh.exe /c:1
IF %ERRORLEVEL% NEQ 0 GOTO QUIT
SBEC_flash_tosh.exe /c:1 /f:..\..\56028828\56028828.bin
IF %ERRORLEVEL% NEQ 0 GOTO QUIT
SBEC_eeflash.exe /C:1 /F:..\..\MCU_eeprom\56028828_eeprom.bin /e
GOTO QUIT

:QUIT
PAUSE